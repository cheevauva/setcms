        <base href="{{ scBaseUrl() }}"/>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
    </head>
    <body>
        <div class="container">
            <header class="blog-header py-3">

            </header>
        </div>

        <main class="container">
            <div class="row g-5">
                <div class="col-md-12">
                    {% block content %}{% endblock %}
                </div>
            </div>
        </main>
    </body>
    {% include scShortPath('assets.twig') %}
</html>
