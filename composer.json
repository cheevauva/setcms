{
    "name": "cheevauva/setcms",
    "description": "SetCMS",
    "type": "project",
    "require": {
        "php": ">=8.4",
        "doctrine/dbal": "4.2.1",
        "twig/twig": "v3.16.0",
        "altorouter/altorouter": "1.1.0",
        "league/commonmark": "2.6.0",
        "laminas/laminas-diactoros": "3.5.0",
        "laminas/laminas-permissions-acl": "2.17.0",
        "laminas/laminas-httphandlerrunner": "2.11.0",
        "psr/http-server-handler": "1.0.2",
        "psr/http-server-middleware": "1.0.2",
        "psr/container": "2.0.0"
    },
    "require-dev": {
        "phpstan/phpstan": "2.1.17",
        "phpunit/phpunit": "12.2.2"
    },
    "autoload": {
        "psr-4": {
            "SetCMS\\": "SetCMS/",
            "UUA\\": "UUA/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SetCMS\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
